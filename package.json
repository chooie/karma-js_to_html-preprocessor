{
  "name": "karma-js_to_html-preprocessor",
  "version": "0.0.2",
  "description": "Karma plugin for @chooie/js_to_html",
  "main": "src/application/shared/main.js",
  "engines": {
    "node": "9.4.0"
  },
  "scripts": {
    "heroku-prebuild": "./tasks.sh build:all",
    "pretty": "prettier --write \"src/**/*.js\"",
    "precommit": "lint-staged && ./tasks.sh test:all loose=true"
  },
  "lint-staged": {
    "*.{js,css}": [
      "npm run pretty",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chooie/karma-js_to_html-preprocessor"
  },
  "author": "Charles Hebert",
  "license": "ISC",
  "devDependencies": {
    "browserify": "16.2.0",
    "chromedriver": "2.37.0",
    "eslint": "4.19.1",
    "husky": "0.14.3",
    "jake": "8.0.16",
    "karma": "2.0.0",
    "karma-commonjs": "1.0.0",
    "karma-mocha": "1.3.0",
    "karma-pug-preprocessor": "1.0.0-beta.2",
    "lint-staged": "7.0.4",
    "mocha": "5.0.5",
    "nodemon": "1.17.3",
    "prettier": "1.11.1",
    "selenium-webdriver": "4.0.0-alpha.1",
    "shelljs": "0.8.1",
    "simplebuild-karma": "1.0.0"
  },
  "dependencies": {
    "@chooie/js_to_html": "1.0.6",
    "express": "4.16.3",
    "procfile": "0.1.1",
    "require-from-string": "^2.0.2"
  }
}
